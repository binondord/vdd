{
  "vm": {
    "ip": "192.168.44.44",
    "memory": "1024",
    "synced_folders": [
      {
        "host_path": "data/",
        "guest_path": "/var/www"
      },
      {
        "host_path": "../../Sites",
        "guest_path": "/var/www/vhosts"
      }

    ],
    "forwarded_ports": []
  },
  "solr" : {
    "version": "4.6.1"
  },
  "varnish": {
    "version": "3.0"
  },
  "vdd": {
    "drush": {
      "version" : "6.5.0"
    },
    "sites": {
      "[shortname]": {
        "account_name": "root",
        "account_pass": "root",
        "account_mail": "box@example.com",
        "site_name": "[shortname]",
        "site_mail": "box@example.com",
        "vhost": {
          "document_root" : "[shortname].dev/docroot",
          "url": "[shortname].dev",
          "alias": ["www.[shortname].dev"]
        }
      }
    }
  }
}
